{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "installCommand": "npm install",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js",
            "methods" : ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}